!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in mxList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'PP')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/MX/${pt.gameCode}.jpg`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- JOKER -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesJoker\"\r\n      ref=\"gamesJoker\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n          style=\"color: white\"\r\n          class=\"modal-title-20 center justify-content-center\"\r\n        >\r\n          JOKER GAMES\r\n        </h4>\r\n        <b-button variant=\"outline-danger\" @click=\"close()\">\r\n          <span class=\"center\">X</span>\r\n        </b-button>\r\n      </template>\r\n      <!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in jokerList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'JOKER')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/JOKER/${pt.gameCode}.jpg`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- PGSOFT -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesPgsoft\"\r\n      ref=\"gamesPgsoft\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n          style=\"color: white\"\r\n          class=\"modal-title-20 center justify-content-center\"\r\n        >\r\n          PGSOFT GAMES\r\n        </h4>\r\n        <b-button variant=\"outline-danger\" @click=\"close()\">\r\n          <span class=\"center\">X</span>\r\n        </b-button>\r\n      </template>\r\n      <!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in pgsoftList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'PGSOFT')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/PGSOFT/${pt.gameCode}.png`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- MICROGAMING -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesMicrogaming\"\r\n      ref=\"gamesMicrogaming\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n          style=\"color: white\"\r\n          class=\"modal-title-20 center justify-content-center\"\r\n        >\r\n          MICROGAMING GAMES\r\n        </h4>\r\n        <b-button variant=\"outline-danger\" @click=\"close()\">\r\n          <span class=\"center\">X</span>\r\n        </b-button>\r\n      </template>\r\n      <!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in mgList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'MICROGAMING')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/MG/${pt.gameCode}.png`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- PLAYNGO -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesPlayngo\"\r\n      ref=\"gamesPlayngo\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n          style=\"color: white\"\r\n          class=\"modal-title-20 center justify-content-center\"\r\n        >\r\n          PLAYNGO GAMES\r\n        </h4>\r\n        <b-button variant=\"outline-danger\" @click=\"close()\">\r\n          <span class=\"center\">X</span>\r\n        </b-button>\r\n      </template>\r\n      <!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in playngoList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'PLAYNGO')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/PLAYNGO/${pt.gameCode}.png`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- ONETOUCH -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesOnetouch\"\r\n      ref=\"gamesOnetouch\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n          style=\"color: white\"\r\n          class=\"modal-title-20 center justify-content-center\"\r\n        >\r\n          ONE TOUCH GAMES\r\n        </h4>\r\n        <b-button variant=\"outline-danger\" @click=\"close()\">\r\n          <span class=\"center\">X</span>\r\n        </b-button>\r\n      </template>\r\n      <!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in otList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'ONETOUCH')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/OT/${pt.gameCode}.png`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- GIOCOPLUS -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesGiocoplus\"\r\n      ref=\"gamesGiocoplus\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n          style=\"color: white\"\r\n          class=\"modal-title-20 center justify-content-center\"\r\n        >\r\n          GIOCO PLUS GAMES\r\n        </h4>\r\n        <b-button variant=\"outline-danger\" @click=\"close()\">\r\n          <span class=\"center\">X</span>\r\n        </b-button>\r\n      </template>\r\n      <!-- Baris 1 -->\r\n      <b-row class=\"center\" style=\"padding: 0px\">\r\n        <b-col v-for=\"pt in giocoList\" :key=\"pt.gameCode\" cols=\"2\">\r\n          <b-img\r\n            loading=\"lazy\"\r\n            fluid-grow\r\n            @click=\"playGamesProduct(pt.gameCode, 'GIOCOPLUS')\"\r\n            class=\"game-img-slot\"\r\n            :src=\"\r\n              require(`@/assets/images/games/games-slot/GIOCO/${pt.gameCode}.png`)\r\n            \"\r\n            alt=\"None\"\r\n          />\r\n        </b-col>\r\n      </b-row>\r\n    </b-modal>\r\n\r\n    <!-- HABANERO -->\r\n    <b-modal\r\n      class=\"center\"\r\n      centered\r\n      size=\"xl\"\r\n      scrollable\r\n      id=\"gamesHabanero\"\r\n      ref=\"gamesHabanero\"\r\n      hide-footer\r\n    >\r\n      <template #modal-header=\"{ close }\" class=\"center\">\r\n        <h4\r\n        