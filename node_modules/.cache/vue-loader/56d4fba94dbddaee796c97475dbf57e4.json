\n                        v-model=\"payload.accountNo\"\r\n                        type=\"number\"\r\n                        placeholder=\"Nomor Rekening Bank*\"\r\n                        size=\"sm\"\r\n                        fluid\r\n                      ></b-form-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                  <b-row class=\"register-form-row\">\r\n                    <b-col col cols=\"12\" md=\"4\" class=\"register-form-label\"\r\n                      >NO. HANDPHONE/WA/LINE</b-col\r\n                    >\r\n                    <b-col col cols=\"12\" md=\"8\" class=\"register-form-input\">\r\n                      <b-form-input\r\n                        v-model=\"payload.phone\"\r\n                        type=\"number\"\r\n                        placeholder=\"No. Handphone/WA/Line*\"\r\n                        size=\"sm\"\r\n                        fluid\r\n                      ></b-form-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                  <b-row class=\"register-form-row\">\r\n                    <b-col col cols=\"12\" md=\"4\" class=\"register-form-label\">\r\n                      USERNAME\r\n                    </b-col>\r\n                    <b-col col cols=\"12\" md=\"8\" class=\"register-form-input\">\r\n                      <b-form-input\r\n                        v-model=\"payload.username\"\r\n                        type=\"text\"\r\n                        placeholder=\"Username*\"\r\n                        @keypress=\"checkAlphaNumeric\"\r\n                        size=\"sm\"\r\n                        fluid\r\n                      ></b-form-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                  <b-row class=\"register-form-row\">\r\n                    <b-col col cols=\"12\" md=\"4\" class=\"register-form-label\">\r\n                      EMAIL\r\n                    </b-col>\r\n                    <b-col col cols=\"12\" md=\"8\" class=\"register-form-input\">\r\n                      <b-form-input\r\n                        v-model=\"payload.email\"\r\n                        type=\"email\"\r\n                        placeholder=\"Alamat Email*\"\r\n                        size=\"sm\"\r\n                        fluid\r\n                      ></b-form-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                  <b-row class=\"register-form-row\">\r\n                    <b-col col cols=\"12\" md=\"4\" class=\"register-form-label\">\r\n                      PASSWORD\r\n                    </b-col>\r\n                    <b-col col cols=\"12\" md=\"8\" class=\"register-form-input\">\r\n                      <b-form-input\r\n                        v-model=\"payload.password\"\r\n                        type=\"password\"\r\n                        placeholder=\"Setidaknya 6 Huruf / Angka untuk Kata Sandi*\"\r\n                        size=\"sm\"\r\n                        fluid\r\n                      ></b-form-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                  <b-row class=\"register-form-row\">\r\n                    <b-col col cols=\"12\" md=\"4\" class=\"register-form-label\">\r\n                      KONFIRMASI PASSWORD\r\n                    </b-col>\r\n                    <b-col col cols=\"12\" md=\"8\" class=\"register-form-input\">\r\n                      <b-form-input\r\n                        v-model=\"payload.conPassword\"\r\n                        type=\"password\"\r\n                        placeholder=\"Masukkan kembali Password*\"\r\n                        size=\"sm\"\r\n                        fluid\r\n                      ></b-form-input>\r\n                    </b-col>\r\n                  </b-row>\r\n                  <b-row class=\"register-form-row-last\">\r\n                    <b-col col cols=\"12\" md=\"4\" class=\"register-form-label\">\r\n                      KODE REFERRAL\r\n              