,CAAA,EAAA;AACA,aAAA,eAAA,GAAA,IAAA;AACA,YAAA,OAAA,GAAA;AACA,UAAA,UAAA,EAAA,QADA;AAEA,UAAA,MAAA,EAAA,KAAA,WAAA,CAAA,kBAFA;AAGA,UAAA,aAAA,EAAA,KAAA,WAAA,CAAA,WAHA;AAIA,UAAA,WAAA,EAAA,KAAA,WAAA,CAAA,SAJA;AAKA,UAAA,MAAA,EAAA,KAAA;AALA,SAAA;AAOA,aAAA,eAAA,GAAA,IAAA;AACA,aAAA,UAAA,CAAA,IAAA,CAAA,cAAA,EAAA,OAAA,EACA,IADA,CACA,YAAA;AACA,UAAA,GAAA,CAAA,MAAA,CAAA;AACA,YAAA,KAAA,EAAA,gBADA;AAEA,YAAA,KAAA,EAAA,6BAFA;AAGA,YAAA,IAAA,EAAA,SAHA;AAIA,YAAA,IAAA,EAAA,SAJA;AAKA,YAAA,YAAA,EAAA,IALA;AAMA,YAAA,GAAA,EAAA,CANA;AAOA,YAAA,gBAAA,EAAA;AAPA,WAAA;;AASA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,sBAAA;;AACA,UAAA,KAAA,CAAA,aAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,GAAA,CAAA;;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,cAAA;;AACA,UAAA,KAAA,CAAA,YAAA,CAAA,KAAA;AACA,SAhBA,EAiBA,KAjBA,CAiBA,UAAA,KAAA,EAAA;AACA,UAAA,GAAA,CAAA,MAAA,CAAA;AACA,YAAA,KAAA,EAAA,cADA;AAEA,YAAA,KAAA,EAAA,mBAFA;AAGA,YAAA,IAAA,EAAA,KAHA;AAIA,YAAA,IAAA,EAAA,OAJA;AAKA,YAAA,YAAA,EAAA,IALA;AAMA,YAAA,GAAA,EAAA,CANA;AAOA,YAAA,gBAAA,EAAA;AAPA,WAAA;;AASA,UAAA,KAAA,CAAA,YAAA,CAAA,KAAA;AACA,SA5BA,EA6BA,OA7BA,CA6BA,YAAA;AACA,UAAA,KAAA,CAAA,eAAA,GAAA,KAAA;;AACA,UAAA,KAAA,CAAA,YAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,eAAA,GAAA,KAAA;AACA,SAjCA;AAkCA,OA5CA,MA4CA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA;AACA,UAAA,KAAA,EAAA,gBADA;AAEA,UAAA,KAAA,EAAA,aAFA;AAGA,UAAA,IAAA,EAAA,uBAHA;AAIA,UAAA,IAAA,EAAA,MAJA;AAKA,UAAA,YAAA,EAAA,IALA;AAMA,UAAA,GAAA,EAAA,CANA;AAOA,UAAA,gBAAA,EAAA;AAPA,SAAA;AASA,aAAA,YAAA,CAAA,KAAA;AACA;AACA;AA3DA;AAlCA,CAAA","sourcesContent":["<script>\r\nimport Vue from \"vue\";\r\nimport { GET_RECENT_TRANSACTION } from \"@/store/constants/actions.type\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      sispayCollapse: false,\r\n      transferCollapse: false,\r\n      antidoubleclick: false,\r\n      payload: {\r\n        cash: 0,\r\n      },\r\n      bankReceiver: {\r\n        accountNo: \"54231245\",\r\n        accountName: \"Annie Gunawan\",\r\n        accountBank: \"BRI\",\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    creditAmount: {\r\n      get() {\r\n        return this.payload.cash / 1000;\r\n      },\r\n      set(val) {\r\n        if (!val) {\r\n          return 0;\r\n        }\r\n      },\r\n    },\r\n  },\r\n  mounted() {\r\n    // this.withdrawalPending\r\n    this.bankReceiver.accountNo = this.currentUser.accountNo;\r\n    this.bankReceiver.accountName = this.currentUser.accountName;\r\n    this.bankReceiver.accountBank = this.currentUser.depositBankAccount;\r\n  },\r\n  methods: {\r\n    withdraw() {\r\n      this.changeLoader(true);\r\n      if (this.creditAmount != 0) {\r\n        this.antidoubleclick = true;\r\n        let payload = {\r\n          MethodType: \"Member\",\r\n          ToBank: this.currentUser.depositBankAccount,\r\n          ToAccountName: this.currentUser.accountName,\r\n          ToAccountNo: this.currentUser.accountNo,\r\n          Amount: this.creditAmount,\r\n        };\r\n        this.antidoubleclick = true;\r\n        this.ApiService.post(\"trx/withdraw\", payload)\r\n          .then(() => {\r\n            Vue.notify({\r\n              group: \"SuccessMessage\",\r\n              title: \"Withdrawal Request Berhasil\",\r\n              text: \"Success\",\r\n              type: \"success\",\r\n              closeOnClick: true,\r\n              max: 1,\r\n              ignoreDuplicates: true,\r\n            });\r\n            this.$store.dispatch(GET_RECENT_TRANSACTION);\r\n            this.refreshWallet();\r\n            this.payload.cash = 0;\r\n            this.$emit(\"refresh-list\");\r\n            this.changeLoader(false);\r\n          })\r\n          .catch((error) => {\r\n            Vue.notify({\r\n              group: \"ErrorMessage\",\r\n              title: \"We Have a problem\",\r\n              text: error,\r\n              type: \"error\",\r\n              closeOnClick: true,\r\n              max: 1,\r\n              ignoreDuplicates: true,\r\n            });\r\n            this.changeLoader(false);\r\n          })\r\n          .finally(() => {\r\n            this.antidoubleclick = false;\r\n            this.changeLoader(false);\r\n            this.antidoubleclick = false;\r\n          });\r\n      } else {\r\n        Vue.notify({\r\n          group: \"WarningMessage\",\r\n          title: \"INFORMATION\",\r\n          text: \"Amount tidak boleh 0!\",\r\n          type: \"warn\",\r\n          closeOnClick: true,\r\n          max: 1,\r\n          ignoreDuplicates: true,\r\n        });\r\n        this.changeLoader(false);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n    \r\n<template>\r\n  <div style=\"background-color: #fff; padding-top: 5px; border-radius: 10px\">\r\n    <b-container fluid style=\"padding: 0%; margin: 0%\">\r\n      <b-row>\r\n        <span style=\"font-size: 14px; font-weight: bold; margin-left: 20px\"\r\n          >Withdrawal</span\r\n        >\r\n      </b-row>\r\n\r\n      <b-card\r\n        class=\"mt-3\"\r\n        style=\"background-color: #d1d1d1; border-radius: 10px\"\r\n      >\r\n        <b-row class=\"mt-3\">\r\n          <b-col cols=\"12\" md=\"2\" align-self=\"center\" style=\"font-weight: bold\"\r\n            >Bank</b-col\r\n          >\r\n          <b-col cols=\"12\" md=\"4\" align-self=\"center\">\r\n            <b-input\r\n              :value=\"`${bankReceiver.accountBank}`\"\r\n              class=\"form-control\"\r\n              readonly\r\n            ></b-input>\r\n          </b-col>\r\n\r\n          <b-col cols=\"12\" md=\"2\" align-self=\"center\" style=\"font-weight: bold\"\r\n            >Bank Branch</b-col\r\n          >\r\n          <b-col cols=\"12\" md=\"4\" align-self=\"center\">\r\n            <b-input\r\n              :value=\"`${bankReceiver.accountBank}`\"\r\n              class=\"form-control\"\r\n              readonly\r\n            ></b-input>\r\n          </b-col>\r\n        </b-row>\r\n\r\n        <b-row class=\"mt-3\">\r\n          <b-col cols=\"12\" md=\"2\" align-self=\"center\" style=\"font-weight: bold\"\r\n            >Account Name</b-col\r\n          >\r\n          <b-col cols=\"12\" md=\"4\" align-self=\"center\">\r\n            <b-input\r\n              :value=\"`${bankReceiver.accountName}`\"\r\n              class=\"form-control\"\r\n              readonly\r\n            ></b-input>\r\n          </b-col>\r\n\r\n          <b-col cols=\"12\" md=\"2\" align-self=\"center\" style=\"font-weight: bold\"\r\n            >Account Number</b-col\r\n          >\r\n          <b-col cols=\"12\" md=\"4\" align-self=\"center\">\r\n            <b-input\r\n              :value=\"`${bankReceiver.accountNo}`\"\r\n              class=\"form-control\"\r\n              readonly\r\n            ></b-input>\r\n          </b-col>\r\n        </b-row>\r\n\r\n        <b-row class=\"mt-3\">\r\n          <b-col cols=\"12\" md=\"2\" align-self=\"center\" style=\"font-weight: bold\"\r\n            >Cash Amount</b-col\r\n          >\r\n          <b-col cols=\"12\" md=\"10\" align-self=\"center\">\r\n            <vue-numeric\r\n              v-model=\"payload.cash\"\r\n              class=\"form-control\"\r\n              :precision=\"0\"\r\n            >\r\n            </vue-numeric>\r\n          </b-col>\r\n\r\n          <!-- <b-col cols=\"2\" align-self=\"center\">Destination Account</b-col>\r\n          <b-col cols=\"3\" align-self=\"center\">\r\n            <b-input\r\n              :value=\"`${bankReceiver.accountBank} / ${bankReceiver.accountName} / ${bankReceiver.accountNo}`\"\r\n              class=\"form-control\"\r\n              readonly\r\n            ></b-input>\r\n          </b-col> -->\r\n        </b-row>\r\n\r\n        <b-row class=\"mt-3\">\r\n          <b-col cols=\"12\" md=\"2\" align-self=\"center\" style=\"font-weight: bold\"\r\n            >Credit Amount</b-col\r\n          >\r\n          <b-col cols=\"12\" md=\"10\" align-self=\"center\">\r\n            <vue-numeric\r\n              v-model=\"creditAmount\"\r\n              class=\"form-control\"\r\n              readonly\r\n              :precision=\"0\"\r\n            ></vue-numeric>\r\n          </b-col>\r\n        </b-row>\r\n\r\n        <b-row class=\"mt-3\" align-h=\"end\">\r\n          <b-col cols=\"12\" md=\"2\">\r\n            <b-button\r\n              :disabled=\"antidoubleclick\"\r\n              style=\"width: 100%\"\r\n              class=\"submit-button-2\"\r\n              @click=\"withdraw\"\r\n            >\r\n              Submit\r\n            </b-button>\r\n          </b-col>\r\n        </b-row>\r\n      </b-card>\r\n\r\n      <b-row class=\"mt-2\">\r\n        <b-col style=\"color: black; font-size: 13px\" cols=\"12\" md=\"4\">\r\n          * Withdrawal value minimal :\r\n          <span>{{ parseFloat(trxLimit.minwd).toLocaleString() }}</span>\r\n        </b-col>\r\n        <b-col style=\"color: black; font-size: 13px\" cols=\"12\" md=\"4\">\r\n          * Withdrawal denomination :\r\n          <span>{{ parseFloat(trxLimit.denomwd).toLocaleString() }}</span>\r\n        </b-col>\r\n        <b-col style=\"color: black; font-size: 13px\" cols=\"12\" md=\"4\">\r\n          * Withdrawal value maximum :\r\n          <span> {{ parseFloat(trxLimit.maxwd).toLocaleString() }} </span>\r\n        </b-col>\r\n      </b-row>\r\n    </b-container>\r\n  </div>\r\n</template>"],"sourceRoot":"src/views/Isobet/Mobile/Wallet/components"}]}